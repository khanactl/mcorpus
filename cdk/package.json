{
  "name": "mcorpus-cdk",
  "version": "1.0.0",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "useTabs": true
  },
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "clean": "rm -rf cdk.out && rm -rf dist && rm -f lib/*.d.ts && rm -f lib/*.js && rm -f bin/*.d.ts && rm -f bin/*.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "aws-cdk": "^2.8.0",
    "prettier": "2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudwatch-actions": "^1.140.0",
    "@aws-cdk/aws-codepipeline": "^1.140.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.140.0",
    "@aws-cdk/aws-ec2": "^1.140.0",
    "@aws-cdk/aws-ecs": "^1.140.0",
    "@aws-cdk/aws-ecs-patterns": "^1.140.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.140.0",
    "@aws-cdk/aws-rds": "^1.140.0",
    "@aws-cdk/aws-ssm": "^1.140.0",
    "@aws-cdk/aws-wafv2": "^1.140.0",
    "@aws-cdk/core": "^1.140.0",
    "@aws-cdk/custom-resources": "^1.140.0",
    "@aws-cdk/pipelines": "^1.140.0",
    "@types/archiver": "^5.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/source-map-support": "^0.5.4",
    "@types/uuid": "^8.3.4",
    "archiver": "^5.3.0",
    "aws-sdk": "^2.1061.0",
    "camelcase": "^6.3.0",
    "fs-extra": "^10.0.0",
    "npm-check-updates": "^12.2.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khanactl/mcorpus"
  },
  "license": "MIT"
}
