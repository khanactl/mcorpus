{
  "name": "mcorpus-cdk",
  "version": "1.0.0",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "useTabs": true
  },
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "clean": "rm -rf cdk.out && rm -rf dist && rm -f lib/*.d.ts && rm -f lib/*.js && rm -f bin/*.d.ts && rm -f bin/*.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "aws-cdk": "^2.19.0",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudwatch-actions": "^1.151.0",
    "@aws-cdk/aws-codepipeline": "^1.151.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.151.0",
    "@aws-cdk/aws-ec2": "^1.151.0",
    "@aws-cdk/aws-ecs": "^1.151.0",
    "@aws-cdk/aws-ecs-patterns": "^1.151.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.151.0",
    "@aws-cdk/aws-rds": "^1.151.0",
    "@aws-cdk/aws-ssm": "^1.151.0",
    "@aws-cdk/aws-wafv2": "^1.151.0",
    "@aws-cdk/core": "^1.151.0",
    "@aws-cdk/custom-resources": "^1.151.0",
    "@aws-cdk/pipelines": "^1.151.0",
    "@types/archiver": "^5.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/source-map-support": "^0.5.4",
    "@types/uuid": "^8.3.4",
    "archiver": "^5.3.0",
    "aws-sdk": "^2.1106.0",
    "camelcase": "^6.3.0",
    "fs-extra": "^10.0.1",
    "npm-check-updates": "^12.5.5",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khanactl/mcorpus"
  },
  "license": "MIT"
